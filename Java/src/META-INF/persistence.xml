<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="wellderly_rules">
	<provider>org.batoo.jpa.core.BatooPersistenceProvider</provider>
	<class>edu.sdsc.wellderly.rules.VCFData</class>
	<class>edu.sdsc.wellderly.rules.VCFComplexData</class>
	<class>edu.sdsc.wellderly.rules.VCFComplexGroup</class>
	<class>edu.sdsc.wellderly.rules.VCFGroup</class>
	<properties>				
